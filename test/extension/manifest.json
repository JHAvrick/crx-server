{
	"manifest_version": 2,
	"name": "Chrome Extension",
	"version": "3.0.7",
	"description": "This is a barebones Chrome extension for demo purposes.",
	"permissions": [
		"http://*/",
		"https://*/"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Click me!"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"popup.js"
			]
		}
	]
}